{
  "name": "@ls-stack/ai",
  "version": "0.2.5",
  "license": "MIT",
  "scripts": {
    "lint": "pnpm tsc",
    "tsc": "tsc -p tsconfig.prod.json",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "build": "pnpm test && pnpm lint && pnpm run build:no-test",
    "build:no-test": "tsup --minify --dts",
    "pre-publish": "./scripts/check-if-is-sync.sh && pnpm build",
    "npm-publish": "pnpm publish --access=public"
  },
  "files": [
    "dist"
  ],
  "repository": "github:lucaslos/ai",
  "author": "Lucas Santos",
  "main": "./dist/main.js",
  "module": "./dist/main.mjs",
  "exports": {
    ".": {
      "import": "./dist/main.mjs",
      "require": "./dist/main.js"
    }
  },
  "engines": {
    "node": ">=23.0.0"
  },
  "devDependencies": {
    "@ls-stack/eslint-cfg": "^0.1.3",
    "@ls-stack/utils": "^2.4.0",
    "@types/node": "^22.13.1",
    "@vitest/ui": "^3.0.5",
    "eslint": "^9.20.0",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.15"
  },
  "peerDependencies": {
    "ai": "^4.1.34"
  }
}
